<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gg.rubit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Rubit"
        android:usesCleartextTraffic="true">

        <activity
            android:name="gg.rubit.Activitys.Lecciones.VideoLecturaActivity"
            android:exported="false" />
        <activity
            android:name=".perfilactivity"
            android:exported="false" />
        <activity
            android:name="gg.rubit.Activitys.Lecciones.ConversacionActivity"
            android:exported="false" />
        <activity
            android:name="gg.rubit.Tutorial.TutorialActivity"
            android:exported="false" />
        <activity
            android:name="gg.rubit.Tutorial.TutorialMainActivity"
            android:exported="false" />
        <activity
            android:name="gg.rubit.Splash.splash_Activity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="gg.rubit.Activitys.MensajeLoginActivity" />
        <activity android:name="gg.rubit.Activitys.Login.RegistrarseActivity" />
        <activity android:name="gg.rubit.Activitys.Login.RecuperarContrasenaActivity" />
        <activity android:name="gg.rubit.Activitys.Login.LoginActivity" />
    </application>
</manifest>